package com.yeelim.learn.ehcache.model;

/**
 * @author Yeelim
 * @date 2014-3-18
 * @time 下午9:05:22 
 *
 */
public class User {
	
	private Integer id;
	private String name;
	private String tel;
	private String mobile;
	private Integer age;
	private Integer sex;
	private Unit unit;
	private User2 user2;
	
	public User() {}
	
	public User(Integer id, String name, Integer age) {
		this.id = id;
		this.name = name;
		this.age = age;
	}
	
	/**
	 * 返回属性id
	 * @return the id
	 */
	public Integer getId() {
		return id;
	}
	/**
	 * 给属性id赋值
	 * @param id the id to set
	 */
	public void setId(Integer id) {
		this.id = id;
	}
	/**
	 * 返回属性name
	 * @return the name
	 */
	public String getName() {
		return name;
	}
	/**
	 * 给属性name赋值
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}
	/**
	 * 返回属性tel
	 * @return the tel
	 */
	public String getTel() {
		return tel;
	}
	/**
	 * 给属性tel赋值
	 * @param tel the tel to set
	 */
	public void setTel(String tel) {
		this.tel = tel;
	}
	/**
	 * 返回属性mobile
	 * @return the mobile
	 */
	public String getMobile() {
		return mobile;
	}
	/**
	 * 给属性mobile赋值
	 * @param mobile the mobile to set
	 */
	public void setMobile(String mobile) {
		this.mobile = mobile;
	}
	/**
	 * 返回属性age
	 * @return the age
	 */
	public Integer getAge() {
		return age;
	}
	/**
	 * 给属性age赋值
	 * @param age the age to set
	 */
	public void setAge(Integer age) {
		this.age = age;
	}
	/**
	 * 返回属性sex
	 * @return the sex
	 */
	public Integer getSex() {
		return sex;
	}
	/**
	 * 给属性sex赋值
	 * @param sex the sex to set
	 */
	public void setSex(Integer sex) {
		this.sex = sex;
	}
	/**
	 * 返回属性unit
	 * @return the unit
	 */
	public Unit getUnit() {
		return unit;
	}
	/**
	 * 给属性unit赋值
	 * @param unit the unit to set
	 */
	public void setUnit(Unit unit) {
		this.unit = unit;
	}
	/**
	 * 返回属性user2
	 * @return the user2
	 */
	public User2 getUser2() {
		return user2;
	}
	/**
	 * 给属性user2赋值
	 * @param user2 the user2 to set
	 */
	public void setUser2(User2 user2) {
		this.user2 = user2;
	}
	
	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("User [id=").append(id).append(", name=").append(name)
				.append(", tel=").append(tel).append(", mobile=")
				.append(mobile).append(", age=").append(age).append(", sex=")
				.append(sex).append(", unit=").append(unit).append(", user2=")
				.append(user2).append("]");
		return builder.toString();
	}

}
